{
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["."],
        "alias": {
          "#assets": "./src/assets",
          "#components": "./src/components",
          "#JobComposer": "./src/JobComposer",
          "#PrototypeComposer": "./src/PrototypeComposer",
          "#mocks": "./mocks",
          "#modules": "./src/modules",
          "#store": "./src/store",
          "#utils": "./src/utils",
          "#views": "./src/views",
          "#services": "./src/services"
        }
      }
    ]
  ]
}
